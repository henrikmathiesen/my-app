{
  "name": "my-app",
  "version": "0.0.0",
  "homepage": "https://henrikmathiesen.github.io/my-app",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --locale=sv",
    "start:henry": "ng serve --env=henry",
    "build:aot": "ng build --aot",
    "build:prod": "ng build --prod",
    "build:prod:gh": "ng build --prod --base-href /my-app/",
    "build:prod:sm": "ng build --prod --sourcemaps=true",
    "serve:build:prod": "http-server ./dist -p 1337",
    "copy:foo": "cpx \"node_modules/foo/skins/**/*\" src/assets/vendor/foo/skins",
    "test": "ng test",
    "lint": "ng lint",
    "deploy": "npm run build:prod:gh&&gh-pages -d dist",
    "clean-gh-pages": "rimraf node_modules/gh-pages/.cache"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.0",
    "@angular/common": "5.2.0",
    "@angular/compiler": "5.2.0",
    "@angular/core": "5.2.0",
    "@angular/forms": "5.2.0",
    "@angular/http": "5.2.0",
    "@angular/platform-browser": "5.2.0",
    "@angular/platform-browser-dynamic": "5.2.0",
    "@angular/router": "5.2.0",
    "@ng-bootstrap/ng-bootstrap": "1.1.2",
    "animate.css": "3.6.1",
    "core-js": "2.4.1",
    "font-awesome": "4.7.0",
    "foundation-sites": "6.4.3",
    "libphonenumber-js": "1.2.15",
    "lodash": "4.17.4",
    "lodash.random": "3.2.0",
    "ngx-pagination": "3.0.3",
    "ngx-pipes": "2.1.0",
    "rxjs": "5.5.6",
    "zone.js": "0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.0",
    "@angular/language-service": "5.2.0",
    "@types/jasmine": "2.8.3",
    "@types/jasminewd2": "2.0.2",
    "@types/lodash": "4.14.74",
    "@types/node": "6.0.60",
    "codelyzer": "4.0.1",
    "gh-pages": "1.1.0",
    "http-server": "0.10.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.2",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.5.3"
  }
